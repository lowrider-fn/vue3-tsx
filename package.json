{
  "name": "vue3-tsx",
  "version": "0.1.0",
  "description": "Vue 3/Vuex/Vue-router/Typescript example",
  "author": {
    "name": "Sergeiy Dolmatskiy",
    "email": "sergey.dolmatskiy@gmail.com"
  },
  "keywords": [
    "vue",
    "vue3 tsx",
    "vue 3",
    "vuex 4",
    "vue typescript",
    "vuex typescript"
  ],
  "scripts": {
    "postinstall": "husky install && chmod ug+x .husky/*",
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint --quiet --fix --ext .js,.ts,.tsx .",
    "stylelint": "stylelint src/**/*.{css,scss} --fix",
    "types": "tsc --noEmit",
    "reinstall": "yarn cache clean --force && rm -rf node_modules yarn.lock && yarn"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "dayjs": "^1.10.7",
    "uuid": "^8.3.2",
    "vue": "^3.0.0",
